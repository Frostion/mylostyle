<config xmlns="http://xmlns.sony.net/mylo/widget" version="1.0">
  <item title="LEDで新着表示" name="led" type="checkbox" comment="チェックすると、新着情報がある場合にステータスインジケーターでお知らせします。">
    <default>on</default>
    <value>on</value>
  </item>
  <item title="更新間隔" name="update" type="select">
    <option value="300000">5分毎</option>
    <option value="600000">10分毎</option>
    <option value="1800000">30分毎</option>
    <option value="3600000">1時間毎</option>
    <option value="7200000">2時間毎</option>
    <option value="-1">自動取得しない</option>
    <default>1時間毎</default>
    <value>1時間毎</value>
  </item>
  <item title="" name="autologin" type="checkbox" comment=""  visible="off">
    <default>off</default>
    <value>off</value>
  </item>
  <item title="" name="encryptEmailPasswd" type="text" comment=""  visible="off">
    <default>,</default>
    <value>,</value>
  </item>
</config>
